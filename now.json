{
    "version": 2,
    "builds": [{ "src": "app.js", "use": "@now/node-server" }],
    "routes": [
      {
        "src": "/",
        "dest": "/app.js",
        "methods": ["GET"]
      },
      {
        "src": "/books",
        "dest": "./api/books.js",
        "methods": ["GET"]
      },
      {
        "src": "/books",
        "dest": ".api/books.js",
        "methods": ["POST"]
      }
    ]
  }